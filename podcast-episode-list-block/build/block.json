{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cpl/podcast-episode-list",
	"version": "0.1.1",
	"title": "Podcast Episode List",
	"category": "widgets",
	"icon": "archive",
	"description": "List some recent posts of a certain post type.",
	"supports": {
		"html": false
	},
	"attributes": {
		"postsToShow": {
			"type": "number",
			"default": 5
		}
	},
	"textdomain": "podcast-episode-list",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
